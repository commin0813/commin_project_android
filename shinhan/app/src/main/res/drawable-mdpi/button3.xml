<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <padding android:bottom="4dp" android:left="8dp" android:right="8dp" android:top="4dp" />
            <solid android:color="#ffffff" />
            <stroke android:width="7px" android:color="#f8f8f8"></stroke>
            <corners android:bottomLeftRadius="12dp" android:bottomRightRadius="12dp" android:topLeftRadius="12dp" android:topRightRadius="12dp" />
        </shape>
    </item>
</selector>